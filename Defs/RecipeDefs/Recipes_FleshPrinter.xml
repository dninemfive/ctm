  <!-- organ recipes -->
  <RecipeDef Name="D9PrintOrgansBase" Abstract="True">
	<jobString>Printing organ.</jobString>
	<workAmount>24000</workAmount>
	<workSpeedStat>MedicalOperationSpeed</workSpeedStat>
	<requiredGiverWorkType>Tailoring</requiredGiverWorkType>
	<effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>D9CTM.IngredientValueGetter_Mass</ingredientValueGetterClass>
	<fixedIngredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
	  <thingDefs>
		<li>D9Nanomaterial</li>
	  </thingDefs>
    </fixedIngredientFilter>
	<workSkill>Medicine</workSkill>
    <researchPrerequisite>D9PrintOrgans</researchPrerequisite>
	<skillRequirements>
		<Medicine>6</Medicine>
		<Intellectual>4</Intellectual>
		<Crafting>13</Crafting>
	</skillRequirements>
  </RecipeDef>
