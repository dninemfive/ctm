		<jobString>Making psychic foil helmet.</jobString>
		<workAmount>3400</workAmount>
		<researchPrerequisite>D9CTM_PsychicTheory</researchPrerequisite>
		<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>D9CTM_PsychicFoil</li>
					</thingDefs>
				</filter>
				<count>24</count>
			</li>
