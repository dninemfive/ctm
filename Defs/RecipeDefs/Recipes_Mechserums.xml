<Defs>
	<RecipeDef>
		<defName>Make_Neurotrainer</defName>
		<label>make blank neurotrainer</label>
		<description>Make a blank neurotrainer.</description>
		<jobString>Making blank neurotrainer.</jobString>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>16000</workAmount>
		<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>D9Nanomaterial</li>
			  </thingDefs>
			</filter>
			<count>80</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>ComponentSpacer</li>
			  </thingDefs>
			</filter>
			<count>3</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>Plasteel</li>
			  </thingDefs>
			</filter>
			<count>40</count>
		  </li>			
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>D9Nanomaterial</li>
			<li>ComponentSpacer</li>
			<li>Plasteel</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <UnprogrammedNeurotrainer>1</UnprogrammedNeurotrainer>
		</products>
		<researchPrerequisite>D9Neurotrainer</researchPrerequisite>
		<skillRequirements>
		  <Crafting>10</Crafting>
		  <Intellectual>8</Intellectual>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
    </RecipeDef>
