<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods><li>[1.0] RePower</li></mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<RePower.RePowerDef>
							<!-- Unique name for the mod-compatiability def -->
							<defName>RePowerD9TableAIProgramming</defName>
							<!-- defName of the building we are looking for to override -->
							<targetDef>TableAIProgramming</targetDef>
							<!-- Power network effect in "low power" mode, which is the default state -->
							<lowPower>-10</lowPower>
							<!-- Power network effect in "high power" mode, note this can be positive (cremate bodies for power?) -->
							<highPower>-900</highPower>
							<!-- If true, set to a high power state, but only works for workbenches. -->
							<poweredWorkbench>true</poweredWorkbench>
							<!-- If true, set to high power state whenever reserved by a colonist, for non-workbenches (like deep drill, powered mortars) -->
							<poweredReservable>false</poweredReservable>
						</RePower.RePowerDef>
						<RePower.RePowerDef>
							<defName>RePowerD9TableNanoforge</defName>
							<targetDef>TableNanoforge</targetDef>
							<lowPower>-10</lowPower>
							<highPower>-900</highPower>
							<poweredWorkbench>true</poweredWorkbench>
							<poweredReservable>false</poweredReservable>
						</RePower.RePowerDef>
						<RePower.RePowerDef>
							<defName>RePowerD9FleshPrinter</defName>
							<targetDef>D9FleshPrinter</targetDef>
							<lowPower>-10</lowPower>
							<highPower>-700</highPower>
							<poweredWorkbench>true</poweredWorkbench>
							<poweredReservable>false</poweredReservable>
						</RePower.RePowerDef>
						<!-- Simpods? Healpods? Field projectors? Dunno, will probably use a custom comp instead -->
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>