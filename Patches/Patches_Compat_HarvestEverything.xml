<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods><li>Harvest Everything!</li></mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<!-- Stomach -->
				<li Class="PatchOperationInsert">
					<xpath>Defs/RecipeDef[defName="Make_Liver_D9"]</xpath>
					<value>
						<RecipeDef ParentName="D9PrintOrgansBase">
							<defName>Make_Stomach_D9</defName>
							<label>make stomach</label>
							<description>Print an organic stomach using animal biomass and nanites.</description>
							<ingredients>
							  <li>
								<filter>
								  <categories>
									<li>MeatRaw</li>
								  </categories>
								</filter>
								<count>2.5</count>
							  </li>
							  <li>
								<filter>
								  <thingDefs>
									<li>D9Nanomaterial</li>
								  </thingDefs>
								</filter>
								<count>1.5</count>
							  </li>
							</ingredients>
							<products>
							  <Stomach>1</Stomach>
							</products>
					    </RecipeDef>
					</value>
				</li>
				<!-- Eye -->
				<li Class="PatchOperationInsert">
					<xpath>Defs/RecipeDef[defName="Make_Stomach_D9"]</xpath>
