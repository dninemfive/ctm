									<li>D9Nanomaterial</li>
								  </thingDefs>
								</filter>
								<count>1</count>
							  </li>
							</ingredients>
							<products>
							  <Eye>1</Eye>
							</products>
					    </RecipeDef>
					</value>
				</li>
