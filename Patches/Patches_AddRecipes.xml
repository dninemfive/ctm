<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- Add recipe for glitterworld medicine -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThingDef[defName="MedicineUltratech"]</xpath>
		<value>
			<costList>
			  <MedicineIndustrial>1</MedicineIndustrial>
			  <D9CTM_Nanomaterial>5</D9CTM_Nanomaterial>
			  <Synthread>3</Synthread>
			</costList>
			<recipeMaker>
			  <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
			  <workSkill>Intellectual</workSkill>
			  <recipeUsers>
				<li>DrugLab</li>
			  </recipeUsers>
			  <researchPrerequisite>D9CTM_GlitterworldMedicine</researchPrerequisite>
			  <skillRequirements>
				<Crafting>6</Crafting>
				<Intellectual>8</Intellectual>
			  </skillRequirements>
			</recipeMaker>
		</value>
	</Operation>
</Patch>